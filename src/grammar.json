{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "autohotkey_v2",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "directive"
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          }
        ]
      }
    },
    "directive": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_clipboard_timeout_directive"
        }
      ]
    },
    "_clipboard_timeout_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "#ClipboardTimeout",
          "flags": "i"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        }
      ]
    },
    "integer": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_decimal_integer"
        },
        {
          "type": "SYMBOL",
          "name": "_hexadecimal_integer"
        }
      ]
    },
    "_decimal_integer": {
      "type": "PATTERN",
      "value": "\\d+",
      "flags": "i"
    },
    "_hexadecimal_integer": {
      "type": "PATTERN",
      "value": "0x\\d+",
      "flags": "i"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}